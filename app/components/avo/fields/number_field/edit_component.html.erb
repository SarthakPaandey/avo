<%= field_wrapper **field_wrapper_args do %>
  <%= @form.number_field @field.id, **field_input_args.merge({
    class: classes("w-full"),
    disabled: disabled?,
    max: @field.max,
    min: @field.min,
    placeholder: @field.placeholder,
    step: @field.step,
    autocomplete: @field.autocomplete
  })%>
<% end %>
